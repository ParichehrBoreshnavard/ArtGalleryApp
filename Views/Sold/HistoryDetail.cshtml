@{

    Layout = "_LayoutAdmin";
}

@model ArtGalleryApp.Models.DataViewModel.HistoryViewModel

<div class="container w-100 d-flex flex-nowrap justify-content-center pt-5 pb-5   AdminBg   ">
    <div class="Carouselcontainer w-90 h-90 bg-white  p-3 d-flex">
        <h3> Informations</h3>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label asp-for="galleryTitle"></label>
                    <input readonly type="text" class="form-control" asp-for="galleryTitle" placeholder="Enter first name...">
                </div>
                <div class="form-group">
                    <label asp-for="ArtistName"></label>
                    <input readonly type="text" class="form-control" asp-for="ArtistName" placeholder="Enter last name...">
                </div>
                <div class="form-group">
                    <label asp-for=soldDate></label>
                    <input readonly type="datetime" class="form-control" asp-for="soldDate" placeholder="Enter your year of birth...">
                </div>

                <div class="form-group">
                    <label asp-for="Price"></label>
                    <input readonly type="text" class="form-control" asp-for="Price" placeholder="Enter your portfolio link...">
                </div>
                <div class="form-group position-relative">
                <img class="w-100 position-relative" src="@Model.imgurl" />
                </div>
            </div>
            <div class="col-md-6 position-relative">
                <div class="form-group">
                    <label asp-for="customerName"></label>
                    <input readonly type="text" class="form-control" asp-for="customerName" placeholder="Enter your portfolio link...">
                </div>
                <div class="form-group">
                    <label asp-for="Email">Phone</label>
                    <input readonly type="text" class="form-control" asp-for="Email" placeholder="Enter your portfolio link...">
                </div>
                <div class="form-group">
                    <label asp-for="State"></label>
                    <input readonly type="text" class="form-control" asp-for="State" placeholder="Enter your portfolio link...">
                </div>
                <div class="form-group">
                    <label asp-for="City"></label>
                    <input readonly type="text" class="form-control" asp-for="City" placeholder="Enter your portfolio link...">
                </div>
                <div class="form-group">
                    <label asp-for="Address"></label>
                    <input readonly type="text" class="form-control" asp-for="Address" placeholder="Enter your portfolio link...">
                </div>
                <div class="form-group">
                    <label asp-for="postalCode"></label>
                    <input readonly type="text" class="form-control" asp-for="postalCode" placeholder="Enter your portfolio link...">
                </div>
                
            </div>

        </div>
        <a href="/Sold/History" class="btn btn-primary">back</a>

    </div>
</div>


@section Scripts{
    <partial name="_ValidationScriptsPartial"></partial>
}
