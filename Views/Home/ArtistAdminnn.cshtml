
@{
    ViewData["Title"] = "ArtistAdminnn";
    Layout = "_Layout";
}

    <div class="container w-100 d-flex flex-nowrap justify-content-center pt-5 pb-5  homeSliderBg  ">
    <div class="Carouselcontainer w-80 h-80 bg-white p-2">
        
            <div class="container ">

    <!-- Artist's Profile Table -->
    <h1 class="mt-4">Artist's Profile</h1>
    <div class="row mt-4">
        <div class="col-md-12">
            <table class="table table-hover table-bordered">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Jane Doe</td>
                        <td>jane.doe@example.com</td>
                        <td>555-1234</td>
                        <td><a href="#" class="btn btn-primary"><i class="fas fa-edit"></i> Edit</a></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <!-- Artwork Details Table -->
    <h1 class="mt-4">Artwork Details</h1>
    <div class="row mt-4">
        <div class="col-md-12">
            <table class="table table-hover table-bordered">
                <thead>
                    <tr>
                        <th>Item</th>
                        <th>Inventory</th>
                        <th>Price</th>
                        <th>Availability</th>
                        <th>Produce Year</th>
                        <th>Medium</th>
                        <th>Style</th>
                        <th>Field</th>
                        <th>Publish Date</th>
                        <th>Sold Date</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Untitled</td>
                        <td>123</td>
                        <td>$500</td>
                        <td>In Stock</td>
                        <td>2021</td>
                        <td>Oil on canvas</td>
                        <td>Abstract</td>
                        <td>Painting</td>
                        <td>2021-01-01</td>
                        <td>-</td>
                        <td><a href="#" class="btn btn-primary"><i class="fas fa-edit"></i> Edit</a></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <!-- Upload New Artwork Link -->
    <div class="mt-4">
        <a href="#" class="btn btn-primary"><i class="fas fa-upload"></i> Upload New Artwork</a>
    </div>
</div>

<!-- Include Bootstrap JS -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com
bootstrap/4.5.2/js/bootstrap.min.js"></script>
<!-- Include Font Awesome JS -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js"></script>
</div>
    </div>



